pipeline {
    agent any

    parameters {
        string(name: 'AMI_ID', defaultValue: '', description: 'id da ami do ubuntu')
    }

    stages {
        stage('Clone repo') {
            steps {
                git url: 'https://github.com/ericohortolan/projetofinalGamaThree', branch: 'main'
            }
        }
        stage('Build Cluster') {
            steps {
                sh "04-pipelineSQL/deploy.sh"
            }
        }
        //stage('Test Cluster') {
        //    steps {
        //        sh '04-pipelineSQL/test.sh'
        //    }
        //}
    }
}
